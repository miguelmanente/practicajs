!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>EDICION DOCENTE</title>

    <link rel="stylesheet" href="https://bootswatch.com/4/lux/bootstrap.min.css">

<body>
    <nav class="navbar navbar-dark bg-dark">
        <a href="/" class="navbar-brand">EDICIÃ“N DATOS DOCENTES</a>
    </nav>

    <div class="container mt-5">
        <div class="row">
            <div class="col-md-5">
                <div class="card">
                    <div class="card-body">
                        <form action="/actualizar/<%= data.idprofesor %>" method="post">
                            <div class="form-group">
                                <input type="text" name="nombre" value="<%= data.nombre %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="apellido" value="<%= data.apellido %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="dni" value="<%= data.dni %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="f_nacimiento" value="<%= data.f_nacimiento %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="email" value="<%= data.email %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="celular" value="<%= data.celular%>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="direccion" value="<%= data.direccion %>" class="form-control">
                            </div>
                            <button type="submit" class="btn btn-primary btn-block">
                                EDITAR DOCENTE
                            </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

</body>

</html>